{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/visual-search.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/index-products.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}